[{
    "title": "NEON Fish Label - Experiments by Laura",
    "displaySpeciesAuthor": 0,
    "displayBarcode": 0,
    "labelType": "2",
    "defaultStyles": "font-size:12pt",
    "defaultCss": "../../css/symb/labelhelpers.css",
    "customCss": "",
    "labelDiv": {
      "className": "font-sans label-md"
    },
    "labelHeader": {
      "prefix": "ASU NEON Biorepository Fish Collection",
      "midText": 0,
      "midStr": "",
      "suffix": "",
      "style": "",
      "className": "font-bold text-base mb-1"
    },
    "labelBlocks": [{
      "divBlock": {
        "className": "label-blocks",
        "style": "min-height: 100%; display: grid;",
        "blocks": [{
            "divBlock": {
              "className": "label-top",
              "blocks": [{
                  "fieldBlock": [{
                    "field": "family",
                    "className": "text-base font-normal mb-1"
                  }]
                },
                {
                  "divBlock": {
                    "className": "taxonomy",
                    "style": "",
                    "delimiter": " ",
                    "blocks": [{
                      "fieldBlock": [{
                          "field": "identificationqualifier"
                        },
                        {
                          "field": "speciesname",
                          "className": "text-2xl italic"
                        },
                        { "field": "parentauthor" },
                        { "field": "taxonrank" },
                        {
                          "field": "infraspecificepithet",
                          "className": "font-bold italic"
                        }
                      ]
                    }]
                  } // End of taxonomy block
                },
                // begins determination
                {
                  "fieldBlock": [{
                      "field": "identifiedby",
                      "prefix": "Det by: "
                    },
                    { "field": "dateidentified" }
                  ]
                },
                { "fieldBlock": [{ "field": "identificationreferences" }] },
                { "fieldBlock": [{ "field": "identificationremarks" }] },
                { "fieldBlock": [{ "field": "taxonremarks" }] },
                // begins cat-nums
                {
                  "divBlock": {
                    "className": "cat-nums bar",
                    "blocks": [{
                      "fieldBlock": [{
                          "field": "catalognumber",
                          "className": "text-2xl font-bold block"
                        },
                        {
                          "field": "othercatalognumbers",
                          "className": "text-lg font-normal block"
                        }
                      ]
                    }]
                  }
                },
                {
                  "divBlock": {
                    "className": "local mt-2",
                    "blocks": [{
                      "fieldBlock": [
                        { "field": "country" },
                        {
                          "field": "stateprovince",
                          "prefix": ", "
                        },
                        { "field": "county", "prefix": ", " },
                        {
                          "field": "municipality",
                          "prefix": ", "
                        },
                        {
                          "field": "locality",
                          "className": "uppercase",
                          "prefix": ": "
                        },
                        {
                          "field": "decimallatitude",
                          "prefix": ": ",
                          "suffix": "° N"
                        },
                        {
                          "field": "decimallongitude",
                          "prefix": " ",
                          "suffix": "° W"
                        },
                        {
                          "field": "coordinateuncertaintyinmeters",
                          "prefix": " +-",
                          "suffix": " meters",
                          "style": "margin-left:10px"
                        },
                        {
                          "field": "elevationinmeters",
                          "prefix": ", ",
                          "suffix": "m."
                        }
                      ]
                    }]
                  }
                }
                // End of label-top
              ]
            }
          },
          {
            "divBlock": {
              "className": "label-bottom",
              "blocks": [{
                  "divBlock": {
                    "className": "collector block",
                    "blocks": [{
                      "fieldBlock": [{
                          "field": "recordedby",
                          "className": "uppercase",
                          "prefix": "<span class=\"normal-case\">Coll.: </span>"
                        },
                        {
                          "field": "preparations",
                          "className": "block",
                          "prefix": "Prep.: "
                        }
                      ]
                    }]
                  }
                },
                {
                  "divBlock": {
                    "className": "event text-base",
                    "blocks": [{
                      "fieldBlock": [{
                          "field": "recordnumber",
                          "className": "",
                          "prefix": "Coll. No: "
                        },
                        {
                          "field": "eventdate",
                          "className": "",
                          "prefix": "Date: "
                        }
                      ]
                    }]
                  }
                }
              ]
            }
          }
        ]
      }
    }],
    "labelFooter": {
      "textValue": "",
      "style": ""
    }
  },
  {
    "title": "Label Format Name",
    "displaySpeciesAuthor": 0,
    "displayBarcode": 0,
    "labelType": "2",
    "defaultStyles": "font-size:8pt",
    "defaultCss": "../../css/symb/labelhelpers.css",
    "customCss": "",
    "labelDiv": { "className": "label-md" },
    "labelBlocks": [{
        "divBlock": {
          "className": "label-header",
          "blocks": [{ "fieldName": "family", "className": "font-bold" }]
        }
      },
      {
        "divBlock": {
          "className": "label-middle",
          "blocks": [{ "fieldName": "speciesname", "className": "italic" }]
        }
      },
      {
        "divBlock": {
          "className": "label-footer",
          "blocks": [{ "fieldName": "recordedby", "className": "" }]
        }
      }
    ]
  },
  {
    "title": "Label Format Name",
    "displaySpeciesAuthor": 0,
    "displayBarcode": 0,
    "labelType": "2",
    "defaultStyles": "font-size:8pt",
    "defaultCss": "../../css/symb/labelhelpers.css",
    "customCss": "",
    "labelDiv": { "className": "label-md" },
    "labelBlocks": [{
        "divBlock": {
          "className": "label-header",
          "blocks": [{
            "fieldBlock": [
              { "field": "family", "className": "" },
              { "field": "catalognumber", "className": "font-bold" }
            ]
          }]
        }
      },
      {
        "divBlock": {
          "className": "label-middle",
          "blocks": [{
            "fieldBlock": [{ "field": "speciesname", "className": "italic" }]
          }]
        }
      },
      {
        "divBlock": {
          "className": "label-footer",
          "blocks": [
            { "fieldBlock": [{ "field": "recordedby", "className": "" }] }
          ]
        }
      }
    ]
  }
]