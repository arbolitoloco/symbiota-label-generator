[{
  "divBlock": {
    "className": "label-blocks",
    "style": "",
    "blocks": [{
        "divBlock": {
          "className": "col-title font-bold text-center",
          "content": "NEON Biorepository Mammal Collection"
        }
      },
      {
        "divBlock": {
          "className": "label-top",
          "style": "padding-right: 2cm;",
          "blocks": [{
              "fieldBlock": [{
                "field": "family",
                "className": "text-sm font-normal mb-1"
              }]
            },
            {
              "divBlock": {
                "className": "taxonomy",
